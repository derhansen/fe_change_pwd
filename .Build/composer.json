{
  "name": "derhansen/fe_change_pwd_build",
  "description": "composer.json for builds",
  "repositories": [
    {
      "type": "path",
      "url": "../"
    }
  ],
  "require": {
    "derhansen/fe_change_pwd": "@dev",
    "typo3/cms-core": "^14.0",
    "ext-curl": "*",
    "ext-pdo": "*",
    "php": ">=8.2"
  },
  "require-dev": {
    "typo3/cms-backend": "^14.0",
    "typo3/cms-frontend": "^14.0",
    "typo3/cms-recordlist": "^14.0",
    "typo3/cms-extbase": "^14.0",
    "typo3/cms-fluid": "^14.0",
    "typo3/cms-install": "^14.0",
    "typo3/cms-composer-installers": "^5.0",
    "typo3/testing-framework": "^9.0",
    "friendsofphp/php-cs-fixer": "^3.12.0",
    "phpstan/phpstan": "2.1.22",
    "phpstan/extension-installer": "^1.1"
  },
  "config": {
    "lock": false,
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "phpstan/extension-installer": true
    }
  }
}
